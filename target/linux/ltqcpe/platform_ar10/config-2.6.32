CONFIG_IFX_PPA_IPv6_ENABLE=y
# CONFIG_CRYPTO_DEV_DEU is not set


# CONFIG_IFX_PCIE_HW_SWAP is not set
CONFIG_IFX_PCIE_2ND_CORE=y
# CONFIG_IFX_PCIE1_HW_SWAP is not set

CONFIG_JFFS2_FS_XATTR=y
CONFIG_JFFS2_CMODE_NONE=y
# CONFIG_JFFS2_CMODE_PRIORITY is not set
CONFIG_SQUASHFS_EMBEDDED=y
# CONFIG_IP_NF_RAW=y
# CONFIG_IP_NF_MATCH_SYNFLOOD is not set

CONFIG_IMQ_BEHAVIOR_AA=y
# CONFIG_IMQ_BEHAVIOR_BA is not set

CONFIG_MTD_UBI_WL_THRESHOLD=256
