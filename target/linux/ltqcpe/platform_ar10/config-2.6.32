CONFIG_AR10=y

#
# AR10 Boards
#
CONFIG_AR10_REF_BOARD=y
CONFIG_CPU_BIG_ENDIAN=y
# CONFIG_CPU_LITTLE_ENDIAN is not set
CONFIG_SYS_SUPPORTS_BIG_ENDIAN=y
CONFIG_HW_HAS_PCI=y
# CONFIG_PCI is not set
#Metalink
CONFIG_PCI_MSI=y
CONFIG_BLK_DEV_RAM_SIZE=6144
CONFIG_IFX_ASC_DEFAULT_BAUDRATE=115200
# CONFIG_IFX_LEDC is not set
# CONFIG_SERIAL_IFX_USIF_UART is not set
# CONFIG_IFX_PMON is not set
# UDP redirect KPI hooks support
CONFIG_IFX_UDP_REDIRECT=y
CONFIG_IFX=y
CONFIG_IFX_ADDON=y
#Metalink
CONFIG_FW_LOADER=y
CONFIG_IFX_PMCU=y
# CONFIG_IFX_PMCU_V1 is not set
CONFIG_IFX_CLOCK_CHANGE=y
CONFIG_IFX_BR_OPT=y
CONFIG_IFX_IGMP_SNOOPING=y
CONFIG_IP_MULTICAST=y
CONFIG_IP_MROUTE=y
# CONFIG_IFX_7PORT_SWITCH is not set
CONFIG_IFX_PPA_QOS=y
CONFIG_IFX_PPA_QOS_WFQ=y
CONFIG_IFX_PPA_QOS_RATE_SHAPING=y
